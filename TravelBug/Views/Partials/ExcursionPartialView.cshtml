@model TravelBug.Models.TravelBugModel.Excursion



<div class="image_container">
    <span class="excursion_title">@Model.Title</span>
    @if (Model.Photo != null && Model.Photo.Count > 0)
    {
        <img id="img" src="@Url.Action("GetUrlFromPictureOfExcursion", "ShowImage",
      new
      {
          excursionID = Model.Id
      })"
             alt="" />
    }
    else
    {
        <img id="img" src="~/Content/images/Tour/camera-photo-min.png" alt=""
             style=" width: 10%; margin: 0 auto 0 auto;" />
    }



</div>

